<%- include('include/adminHeader') -%>

<div class="interest-page-dashboard">
  <div class="container">

        <div class="row">
          <div class="user-details">
              <div class="d-flex flex-row justify-content-center">
                <div class="" style="padding: 5px;">
                  <a class="btn btn-dark btn-sm" href="/adashboard"><i class="fa-solid fa-arrow-left fa-lg"></i> Back</a>
                </div>
              </div>
          </div>
        </div>

    <div class="row">
      <div class="col-sm-12">
        <div class="interest-page-bar">
          <br>
          <h6 class="text-center">Used and available access codes</h6>
          <br>
          <!-- <p class="text-center">Data of form filled by people interested in partnership</p> -->
            <div class="interest-table">
              <table class="table table-striped table-hover ">
                <thead>
                  <tr>
                    <th scope="col">SN</th>
                    <th scope="col">Codes</th>
                    <th scope="col">Used by</th>
                    <th scope="col">Code status</th>
                  </tr>
                </thead>
                <tbody>
                  <% for (i=0; i<codes.length; i++){%>
                      <tr>
                        <td><%=i+1%></td>
                        <td><%=codes[i].code%></td>
                        <td><%=codes[i].userMail%></td>
                        <td><%=codes[i].status%></td>
                      </tr>
                  <%}%>
                </tbody>
              </table>
            </div>
        </div>
      </div>
    </div>

  </div>
</div>

<%-include('include/adminFooter') -%>
<%-include('include/footer') -%>